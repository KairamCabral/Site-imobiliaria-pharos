{
  "name": "imobiliaria-pharos",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev -p 3600",
    "dev:3000": "next dev",
    "typecheck": "node --max-old-space-size=2048 ./node_modules/typescript/bin/tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=2048 npm run typecheck && cross-env NODE_OPTIONS=--max-old-space-size=2048 next build",
    "build:server": "NODE_OPTIONS=--max-old-space-size=2048 npm run typecheck && NODE_OPTIONS=--max-old-space-size=2048 next build",
    "start": "next start -p ${PORT:-3600}",
    "test:health": "curl http://localhost:3600/api/health",
    "test:properties": "curl http://localhost:3600/api/properties?limit=5",
    "health-check:fotos": "node scripts/health-check-fotos.js",
    "lint": "next lint",
    "lighthouse": "npm run lighthouse:mobile && npm run lighthouse:desktop",
    "lighthouse:mobile": "lighthouse http://localhost:3600 --only-categories=performance,accessibility,seo --preset=mobile --output=json --output-path=./.reports/lighthouse-mobile.json",
    "lighthouse:desktop": "lighthouse http://localhost:3600 --only-categories=performance,accessibility,seo --preset=desktop --output=json --output-path=./.reports/lighthouse-desktop.json",
    "check:budgets": "node scripts/check-performance-budgets.js",
    "check:a11y": "lighthouse http://localhost:3600 --only-categories=accessibility --output=json --output-path=./.reports/accessibility.json"
  },
  "dependencies": {
    "@googlemaps/markerclusterer": "^2.6.2",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-tabs": "^1.1.13",
    "@types/leaflet": "^1.9.21",
    "@types/leaflet.markercluster": "^1.5.4",
    "autoprefixer": "^10.4.16",
    "class-variance-authority": "^0.7.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.24",
    "ioredis": "^5.8.2",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "leaflet": "^1.9.4",
    "lru-cache": "^11.2.4",
    "lucide-react": "^0.545.0",
    "next": "^15.3.2",
    "next-intl": "^4.1.0",
    "postcss": "^8.4.31",
    "postcss-import": "^15.1.0",
    "react": "^19.1.0",
    "react-countup": "^6.5.3",
    "react-dom": "^19.1.0",
    "react-intersection-observer": "^9.13.1",
    "react-leaflet": "^5.0.0",
    "leaflet.markercluster": "^1.5.3",
    "swiper": "^12.0.2",
    "tailwind-merge": "^3.3.0",
    "tailwind-scrollbar": "^3.0.5",
    "tailwindcss": "^3.3.5",
    "web-vitals": "^4.2.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@types/google.maps": "^3.58.1",
    "@types/jest": "^29.5.12",
    "@types/node": "22.15.19",
    "@types/react": "19.1.4",
    "@types/react-dom": "^19.1.4",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.1",
    "jest": "^29.7.0",
    "lighthouse": "^12.2.0",
    "postcss-custom-properties": "^13.3.2",
    "postcss-nested": "^6.0.1",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.0.0 <21.0.0",
    "npm": ">=10.0.0"
  }
}
